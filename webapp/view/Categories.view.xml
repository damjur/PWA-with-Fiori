<mvc:View controllerName="pl.capgemini.damjur.pwatesteditable.controller.Categories"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
            <List items="{/Categories}" growing="true">
                <ObjectListItem title="{Name}" press="toCategory" type="Navigation"/>
            </List>
        </content>
    </Page>
</mvc:View>
